{
  "background_page": "background.html",
  "content_scripts": [ {
      "all_frames": true,
      "js": [ "inc/jquery-1.3.2.min.js", "inc/jquery-special-scroll.js", "edropper.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],

  "name": "Eye Dropper",
    "version": "0.1",
    "description": "Eye Dropper for Google Chrome.",
    "icons": {
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "EyeDropper",
      "popup": "popup.html"
    },
   "permissions": [ "tabs" ]
}
